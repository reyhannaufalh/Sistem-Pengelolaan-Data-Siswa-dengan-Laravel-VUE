ername</label>\r\n\r\n                <input type=\"text\" class=\"form-control\" v-model=\"post.username\" placeholder=\"Masukkan username Anda\" />\r\n                <!-- validation -->\r\n\r\n                <div v-if=\"validation.username\" class=\"mt-2 alert alert-danger\">\r\n                  {{ validation.username[0] }}\r\n                </div>\r\n              </div>\r\n              <div class=\"form-group mb-5\">\r\n                <label for=\"title\" class=\"font-weight-bold mb-2\">Password</label>\r\n\r\n                <input type=\"password\" class=\"form-control\" v-model=\"post.password\" placeholder=\"Masukkan password Anda\" />\r\n                <!-- validation -->\r\n\r\n                <div v-if=\"validation.password\" class=\"mt-2 alert alert-danger\">\r\n                  {{ validation.password[0] }}\r\n                </div>\r\n              </div>\r\n              <button @click=\"login\" class=\"btn btn-primary w-100\">Login</button>\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Login\",\r\n\r\n  data() {\r\n    return {\r\n      user: {\r\n        email: null,\r\n        password: null,\r\n      },\r\n      loading: false,\r\n      error: null,\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    async login() {\r\n      this.error = null;\r\n\r\n      try {\r\n        await this.$store.dispatch(\"login\", this.user);\r\n        await this.$router.push({ name: \"UserPost\" });\r\n      } catch (error) {\r\n        this.error = error;\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}